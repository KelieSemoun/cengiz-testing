{
    "java.compile.nullAnalysis.mode": "automatic",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**node_modules": true,
        "[!b]*/**": true,
        "b[!a]*/**": true,
        "ba[!c]*/**": true,
        "bac[!k]*/**": true
    },
    "java.debug.settings.onBuildFailureProceed": true,
    "java.configuration.updateBuildConfiguration": "interactive",
